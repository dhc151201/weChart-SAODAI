.status-box{
  width: 120rpx;
}
.tabs-bar::after{
  content: "";
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 0;
  border-bottom: 1rpx solid rgb(224,224,224);
}


.goods-block{
  margin: 40rpx 0;
}

.fixed-item{
  height: 108rpx;
  line-height: 108rpx;
  width: 25%;
  text-align: center;
  /* color: rgb(177, 177, 177); */
}
.fixed-item.bg-success-ease{
  border-radius: 0;
  color: #fff;
}
.fixed-item.bg-summary{
  font-size: 32rpx;
  background: rgb(224,224,224);
  width: 240rpx;
  color: #fff;
  height: 108rpx;
}


.info-block{
  padding-left: 24rpx;
  position: relative;
}
.info-block .title{
  position: relative;
}
.info-block .title::before{
  content: "";
  display: block;
  position: absolute;
  left: -24rpx;
  top: 16rpx;
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
  background: rgb(255, 172, 26);
}
.info-block .content{
  color: #666;
}
view>.info-block:not(:last-child)::after{
  content: "";
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  width: calc(100% + 32rpx);
  border-top: 1rpx solid rgb(224,224,224);
}

.time-worker{
  width: 50%;
  line-height: 2;
}

.shop-image-list{
  width: calc(33% - 6rpx);
  height: 200rpx;
  border-radius: 10rpx;
  margin-bottom: 20rpx; 
}

@-webkit-keyframes yaobai{
  10%{
    transform: rotate(15deg)
  }
  20%{
    transform: rotate(-10deg)
  }
  30%{
    transform: rotate(5deg)
  }
  40%{
     transform: rotate(-5deg)
  }
  50%, 100%{
     transform: rotate(0deg)
  }
}

.status-box-image{
  width: 112rpx;
  height: 94rpx;
  right: 40rpx;
  top: 0;
  animation: yaobai 5s infinite ease-in-out;
  transform-origin: center top;
}