page{
    padding: 0 0 0 32rpx;
    box-sizing: border-box;
    background: #fff;
}
.com-name{
    margin: 40rpx 0 15rpx;
    font-size: 40rpx;
    font-weight: bold;
    position: relative;
}
.com-name::before{
    content: "";
    display: block;
    top: -20rpx;
    width: 100%;
    border-top: 1rpx solid #bbb;
}
sd-button{
    margin: 20rpx 0;
}
.self-hr{
    margin: 24rpx;
}