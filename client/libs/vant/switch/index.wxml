<view
  style="{{ style }}"
  class="custom-class {{ classes }}"
  bind:tap="onClick"
>
  <text class="label {{ classes }}">
    <slot></slot>
  </text>
  <view class="van-switch__node node-class">
    
    <van-loading wx:if="{{ loading }}" size="50%" custom-class="van-switch__loading" />
  </view>
</view>
